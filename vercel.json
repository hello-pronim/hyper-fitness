{
  "version": 2,
  "builds": [
    {
      "src": "./server/index.cjs",
      "use": "@vercel/node",
      "config": {
        "experimental-specifier-resolution": "node"
      }
    },
    {
      "src": "./build",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "./server/index.cjs"
    }
  ]
}
